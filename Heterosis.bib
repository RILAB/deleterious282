	Doi = {10.1371/journal.pgen.1000183},
	Journal = {PLoS Genet},
	Journal-Full = {PLoS genetics},
	Mesh = {Base Sequence; Binding Sites; Gene Conversion; Gene Rearrangement; Genome, Fungal; Molecular Sequence Data; Phylogeny; Polymorphism, Single Nucleotide; Quercus; Saccharomyces cerevisiae; Selection, Genetic; Sequence Alignment; Transcription Factors; Vitis},
