	Mesh = {Genotype; High-Throughput Nucleotide Sequencing; Hordeum; Polymerase Chain Reaction; Polymorphism, Single Nucleotide; Sequence Tagged Sites; Zea mays},
	Number = {5},
	Pages = {e19379},
	Pmc = {PMC3087801},
	Pmid = {21573248},
