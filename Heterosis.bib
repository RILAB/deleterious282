	Mesh = {Crosses, Genetic; Gene Expression Profiling; Gene Expression Regulation, Plant; Genotype; Hybrid Vigor; Hybridization, Genetic; Linear Models; Models, Statistical; Oligonucleotide Array Sequence Analysis; Phenotype; Zea mays},
	Number = {10},
