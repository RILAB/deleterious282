	Mesh = {Amino Acid Substitution; Animals; Arabidopsis; Capsella; Genome, Plant; Humans; Molecular Sequence Data; Mutation; Polymorphism, Genetic; Population Density; Selection, Genetic; Sequence Analysis, DNA},
